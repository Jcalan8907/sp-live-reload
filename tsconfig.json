{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "skipLibCheck": true,
    "outDir": "tmp",
    "types": ["node", "sharepoint"]
  },
  "exclude": ["node_modules"]
}
